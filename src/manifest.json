{
  "name": "Pure New Tab",
  "description": "Change new tab to a pure kikotoko.",
  "manifest_version": 3,
  "icons": {
    "16": "icon/16.png",
    "32": "icon/32.png",
    "48": "icon/48.png"
  },
  "background": {
    "{{chrome}}.service_worker": "src/background.ts",
    "{{firefox}}.scripts": ["src/background.ts"]
  },
  "chrome_url_overrides": {
    "newtab": "src/newtab/newtab.html"
  }
}
